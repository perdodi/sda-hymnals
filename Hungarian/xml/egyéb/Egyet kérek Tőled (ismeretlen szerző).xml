  <properties>
    <titles>
      <title>Egyet kérek Tőled</title>
    </titles>
    <authors>
      <author>ismeretlen szerző</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="i1">
      <lines>Egyet kérek Tőled</lines>
    </verse>
    <verse name="v1">
      <lines>Egyet kérek Tőled, óh Uram, ezt keresem életemben, hogy én ott legyek az Úr házában életem végéig.</lines>
    </verse>
    <verse name="c1">
      <lines>Csodálom az Úr szépségét, keresem az Úr jóságát.</lines>
    </verse>
  </lyrics>
</song>
