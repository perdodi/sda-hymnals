  <lyrics>
    <verse name="i1">
      <lines>Ti, Úrnak szolgái<br/>(Uram, te hívtál)</lines>
    </verse>
    <verse name="v1">
      <lines>Ti, Úrnak szolgái mind! Áldjátok az Urat! Kik az Úr házában álltok éjelente, áldjátok Őt!</lines>
