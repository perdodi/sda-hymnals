<?xml version='1.0' encoding='UTF-8'?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4.6" modifiedIn="OpenLP 2.4.6" modifiedDate="2017-10-01T14:14:09">
  <properties>
    <titles>
      <title>Jöjj, térj meg Jézushoz</title>
    </titles>
    <authors>
      <author>ismeretlen szerző</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="i1">
      <lines>Jöjj, térj meg Jézushoz</lines>
    </verse>
    <verse name="v1">
      <lines>Jöjj, térj meg Jézushoz, add néki szívedet! Jöjj, bízál benne, Megváltód szeret!</lines>
    </verse>
    <verse name="v2">
      <lines>Hallgasd szelíd szavát, fogadd el hívását, Ragadd meg szent kezét, msot nyújtja feléd.</lines>
    </verse>
  </lyrics>
</song>
