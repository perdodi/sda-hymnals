  <properties>
    <titles>
      <title>Méltó, méltó, méltó a Bárány</title>
    </titles>
    <authors>
      <author>ismeretlen szerző</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="i1">
      <lines>Méltó, méltó, méltó a Bárány</lines>
    </verse>
    <verse name="o1">
      <lines>Méltó, méltó, méltó a Bárány,<br/>Méltó, méltó, méltó a Bárány.<br/>Méltó a Bárány, hogy vegyen erőt,<br/>Tisztességet és imádást.<br/>Előtte népek hajolnak meg,<br/>Országa örökké áll.</lines>
    </verse>
  </lyrics>
</song>
