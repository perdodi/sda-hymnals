    <titles>
      <title>Ne félj, ne aggódj</title>
    </titles>
    <authors>
      <author>ismeretlen szerző</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="i1">
      <lines>Ne félj, ne aggódj</lines>
    </verse>
    <verse name="o1">
      <lines>Ne félj, ne aggódj, ne sírj, ne bánkódj: ha tiéd Isten tiéd már minden. Ne félj, ne aggódj, ne sírj, ne bánkódj: elég Ő néked.</lines>
    </verse>
  </lyrics>
