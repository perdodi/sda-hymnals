<?xml version='1.0' encoding='UTF-8'?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4.6" modifiedIn="OpenLP 2.4.6" modifiedDate="2017-10-01T14:13:51">
  <properties>
    <titles>
      <title>Hozsánna!</title>
    </titles>
    <verseOrder>i1 v1 c1 v2 c1 v3 c1</verseOrder>
    <authors>
      <author>ismeretlen szerző</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="i1">
      <lines>Hozsánna!</lines>
    </verse>
    <verse name="v1">
      <lines>Hozsánna! Hozsánna! <br/>Hozsánna Dávid fiának!</lines>
    </verse>
    <verse name="c1">
      <lines>Hozzád száll énekünk,<br/>Szívünkből szeretünk:<br/>Nincs kőszikla senki más,<br/>Csak te vagy a mi Istenünk!</lines>
    </verse>
    <verse name="v2">
      <lines>Dicsőség! Dicsőség! <br/>Dicsőség a feltámadott Úrnak!</lines>
    </verse>
    <verse name="v3">
      <lines>Jézus! Jézus! <br/>Isten Báránya!</lines>
    </verse>
  </lyrics>
</song>
